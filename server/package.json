{
  "name": "auth-graphql",
  "version": "1.0.0",
  "description": "authentication app with graphql",
  "main": "server.js",
  "nodemonConfig": {
    "watch": [
      "*"
    ],
    "ext": "ts,json",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/server.ts",
    "test": "jest",
    "start": "node ./build/src/server.js"
  },
  "postinstall": "npm run build",
  "keywords": [
    "auth",
    "graphql"
  ],
  "author": "Kosam",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
