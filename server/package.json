{
  "name": "auth-graphql",
  "version": "1.0.0",
  "description": "authentication app with graphql",
  "main": "server.js",
  "nodemonConfig": {
    "watch": [
      "*"
    ],
    "ext": "ts,json",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/server.ts",
    "test": "jest",
    "startServer": "node ./build/server.js",
    "client": "npm start --prefix ../client",
    "start": "concurrently \"npm run startServer\" \"npm run client\""
  },
  "postinstall": "npm run build",
  "keywords": [
    "auth",
    "graphql"
  ],
  "author": "Kosam",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.29.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
